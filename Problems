//To add two numbers in c++

#include<iostream>
using namespace std;
int main()
{
    int T;
    cin>>T;
    for(int i=0;i<T;i++)
      { int a,b; cin>>a,b;
      cout<<a+b<<endl;
      }
}

// To calculate area of rectangle

#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
    cout<<a * 1LL * b<<endl;
}

// To iterate each element of string
#include<bits/stdc++.h>
using namespace std;
int main(){
    string str="ehruv";
    str[0]='d';
    for(int i=0; i<str.size(); ++i){
        cout<<str[i]<<endl;
    }
    
}

