// To calculate the maximum subarray sum 
 Given array[2,-4,9,3,-9]

<Algorithm>

int best = 0;
for (int a = 0; a<n; a++){
	sum = 0;
	for(int i = 0; i<n; i++){
	sum += array[i];
	best = max(best,sum);
}
}
cout << best << "\n";	// time complexity of this algorithm is O(n*n)


// Friend  class and function
// stack intializtion *temp


#include <iostream>
using namespace std;
class stack{ private:
    char data_[10]; int top_;
public:
    void init() {top_ =-1;}
    char top() { return data_[top_];}
    void pop() { --top_;}
    void push()(char x) {data_[++top_] = x;}
    int empty() {return (top_ == -1);
	}

}
 
 int main() 
{
     char str[10] = "vurhd";
     stack s; s.init();
     for (int i =0; i<5; ++i) s.push(str[i]);
     while(!s.empty()) { cout << s.top();s.pop();}
     
 }


// To implement vector class 
// Increasing array problem

#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++)
        cin >> v[i];
    long long ans = 0;
    for (int i = 1; i < n; i++)
    {
        if (v[i] < v[i - 1])
            ans += v[i - 1] - v[i], v[i] = v[i - 1];
    }
    cout << ans;
}

