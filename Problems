// To exponent power of a number and find result

#include<iostream>
using namespace std;

double power(double,int=1);
int main(){
    int p;
    double x,y;
    cout<<"enter number:";
    cin>>x;
    cout<<"enter exponent:";
    cin>>p;
    y=power(x,p);
    cout<<"result is:"<<y;
    y=power(x);
    cout<<"\nresult without passing exponent is"<<y;
    
    return 0;
}
double power(double a,int b)
{
double x=1;
for(int i=0; i<b; i++)
x = x*a;
return x;
}


// To add numbers using function
#include<bits/stdc++.h>
using namespace std;

int sum(int n)
{
    int sum = 0;
    while(n){
	sum = sum + n%10;
n= n/10;
}
return sum;}
int main()
{
int a,b;
cin>>a>>b;
cout<<sum(a)<<sum(b);}


		
